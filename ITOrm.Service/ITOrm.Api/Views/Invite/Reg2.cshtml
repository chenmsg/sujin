@model ResultModelData<Users>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>注册</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta name="format-detection" content="telephone=no" />

    <link href="/Themes/css/base.css" rel="stylesheet" />
    <link href="/Themes/css/suMoney.css" rel="stylesheet" />
    <script src="/Themes/js/jquery.js"></script>
    <style>
        html {
            background: url(/Themes/img/suMoney-banner.jpg) no-repeat;
            background-size: 100% 105%;
        }
    </style>
</head>
<body>
    <div class="suMoney-reg-wrap">
        <img src="/Themes/img/suMoney-logo.png" class="suMoney-logo">
        <div class="suMoney-inputBody">
            <div class="suMoney-inputBox">
                <input type="text" placeholder="手机号" class="phone" />
            </div>
            <div class="suMoney-inputBox">
                <input type="text" placeholder="图形验证码" class="yzm" />
                <img src="/Themes/img//yzm.jpg" class="img-yzm">
            </div>
            <div class="suMoney-inputBox">
                <input type="text" placeholder="验证码" class="yzm" />
                <input type="button" id="btn" value="重新获取" class="btn" onclick="time(this)">
            </div>
            <div class="suMoney-inputBox">
                <input type="text" placeholder="密码" class="phone" />
            </div>
            <div class="suMoney-join-info">
                <p>邀请码：<i>@(Model.Data.Mobile)</i></p>
                <a class="suMoney-join-btn">立即加入</a>
            </div>
            <div class="suMoney-copyRight">
                版权所有©2018<br>北京苏津科技有限公司
            </div>

        </div>
    </div>
    <!----占行--->
    <div style="width:100%;height:.2rem;"></div>
</body>
</html>