@model  ITOrm.Payment.Yeepay.respTransferQueryModel
@{
    ViewBag.Title = "结算记录查询";
}

<form>

    <div class="form-group">
        <label for="exampleInputEmail1">结算流水号</label>
        <input type="text" class="form-control" name="externalNo" value="@TQuery.GetSafeString("externalNo")" placeholder="结算流水号">
    </div>
    <button type="submit" class="btn btn-default">提交</button>
</form>
<div class="alert alert-block alert-info fade in">
    <button data-dismiss="alert" class="close" type="button">×</button>
    <h4 class="alert-heading">查询消息:</h4>
    <p>@(Model.message)</p>

</div>


@if (Model.backState == 0)
{
    foreach (var item in Model.transferRequests)
    {

        <div class="alert alert-block alert-info fade in">
            <button data-dismiss="alert" class="close" type="button">×</button>
            <h4 class="alert-heading">查询信息：</h4>
            <p>结算流水号:@(item.batchNo)</p>
            <p>结算状态:@(item.transferStatus)</p>
            <p>子商户编号:@(item.customerNumber)</p>
            <p>结算请求号:@(item.requestNo)</p>
            <p>请求时间:@(item.requestTime)</p>
            <p>处理时间:@(item.handleTime)</p>
            <p>收款人:@(item.receiver)</p>
            <p>收款卡号:@(item.receiverBankCardNo)</p>
            <p>收款银行:@(item.receiverBank)</p>
            <p>金额:@(item.amount)</p>
            <p>手续费:@(item.fee)</p>
            <p>基本手续费:@(item.basicFee)</p>
            <p>额外手续费:@(item.extTargetFee)</p>
            <p>实收金额:@(item.actualAmount)</p>
            <p>失败原因:@(item.failReason)</p>
        </div>
    }
}



<div class="alert alert-block alert-error fade in">
    <button data-dismiss="alert" class="close" type="button">×</button>
    <h4 class="alert-heading">返回报文</h4>
    <p>@(JsonConvert.SerializeObject(Model))</p>

</div>
