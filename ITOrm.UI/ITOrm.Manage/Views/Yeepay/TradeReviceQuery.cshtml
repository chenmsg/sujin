@model respTradeReviceQueryModel
@{
    ViewBag.Title = "交易查询接口";
}


<form>

    <div class="form-group">
        <label for="exampleInputEmail1">支付流水号</label>
        <input type="text" class="form-control" name="requestId" value="@TQuery.GetSafeString("requestId")" placeholder="支付流水号">
    </div>
    <button type="submit" class="btn btn-default">提交</button>
</form>
<div class="alert alert-block alert-info fade in">
    <button data-dismiss="alert" class="close" type="button">×</button>
    <h4 class="alert-heading">查询消息:</h4>
    <p>@(Model.message)</p>

</div>


@if (Model.backState == 0)
{
    foreach (var item in Model.tradeReceives)
    {

        <div class="alert alert-block alert-info fade in">
            <button data-dismiss="alert" class="close" type="button">×</button>
            <h4 class="alert-heading">查询信息：</h4>
            <p>子商户编号:@(item.customerNumber)</p>
            <p>收款宝流水号:@(item.externalId)</p>
            <p>请求时间:@(item.createTime)</p>
            <p>支付时间:@(item.payTime)</p>
            <p>订单金额:@(item.amount)</p>
            <p>手续费:@(item.fee)</p>
            <p>订单状态:@(item.status)</p>
            <p>银行编码:@(item.bankCode)</p>
            <p>持卡人姓名:@(item.payerName)</p>
            <p>银行预留手机号:@(item.payerPhone)</p>
            <p>银行卡后四位:@(item.lastNo)</p>
            <p>结算状态:@(item.withdrawStatus)</p>
        </div>
    }
}



<div class="alert alert-block alert-error fade in">
    <button data-dismiss="alert" class="close" type="button">×</button>
    <h4 class="alert-heading">返回报文</h4>
    <p>@(JsonConvert.SerializeObject(Model))</p>

</div>


