        //    var list = $('.level1 ul li');
        //    $(list).each(function (i) {
        //        rightanimate($(this), i * 100);
        //    })
        //})
    })
    function leftanimate(obj, time)
    {
        setTimeout(function () {
            obj.animate({ left: '0px' });
        },time)
    }
    function rightanimate(obj, time) {
        setTimeout(function () {
            obj.animate({ left: '0px' });
        }, time)
    }
</script>