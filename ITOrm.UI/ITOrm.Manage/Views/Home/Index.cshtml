<script type="text/javascript">
    $(function () {

        $('.level1 ul li').each(function (i) {
            leftanimate($(this), i * 100);
        })
        //菜单点击
        $(".level1 ul li").click(function () {
            $('.level1 ul li').each(function (i) {
                $(this).removeClass("active")
            })
            $(this).addClass("active");
        })
